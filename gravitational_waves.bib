@article{mooley2018mildly,
  title={A mildly relativistic wide-angle outflow in the neutron-star merger event GW170817},
  author={Mooley, KP and Nakar, E and Hotokezaka, K and Hallinan, G and Corsi, A and Frail, DA and Horesh, A and Murphy, T and Lenc, E and Kaplan, DL and others},
  journal={Nature},
